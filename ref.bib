% .bib es un archivo de texto que contiene la bibliografía en formato BibTeX (https://es.wikipedia.org/wiki/BibTeX)

% Para citar un artículo, primero debes buscar su DOI (Digital Object Identifier) en Google Scholar (https://scholar.google.com/), luego copiarlo y pegarlo en la base de datos de Scopus (https://www.scopus.com/), y finalmente exportar la referencia en formato BibTeX.

% para citar un sitio web, puedes usar la siguiente plantilla:
% misc{sitio_web,
%     author = {Autor},
%     title = {Título},
%     howpublished = {\url{https://www.ejemplo.com/}},
%     year = {2021},
%     note = {Último acceso: 2021-01-01}
% }

@misc{latex_wikipedia,
    author = {{Wikipedia contributors}},
    title = {LaTeX},
    howpublished = {\url{https://en.wikipedia.org/wiki/LaTeX}},
    year = {2023},
    note = {Último acceso: 2023-10-01}
}

% ejemplo de ref.bib de einstein:
@article{einstein,
    author = {Albert Einstein},
    title = {Zur Elektrodynamik bewegter K{\"o}rper},
    journal = {Annalen der Physik},
    volume = {322},
    number = {10},
    pages = {891--921},
    year = {1905},
    doi = {10.1002/andp.19053221004}
}

% ejemplo de ref.bib de newton:
@article{newton,
    author = {Isaac Newton},
    title = {Philosophi{\ae} Naturalis Principia Mathematica},
    journal = {Royal Society},
    volume = {1687},
    number = {3},
    pages = {1--2},
    year = {1687},
    doi = {10.1002/andp.19053221004}
}
